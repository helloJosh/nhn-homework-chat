    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "LogFileUtility",
            "request": "launch",
            "mainClass": "com.nhnacademy.util.LogFileUtility",
            "projectName": "chatapplication"
        },
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "mainClass": "com.nhnacademy.Main",
            "projectName": "chatapplication"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "ClientMain",
            "request": "launch",
            "mainClass": "com.nhnacademy.client.ClientMain",
            "projectName": "chatapplication",
            "args": [
                "-i",
                "3"
            ]
        },
        {
            "type": "java",
            "name": "ServerMain",
            "request": "launch",
            "mainClass": "com.nhnacademy.server.ServerMain",
            "projectName": "chatapplication"
        }
    ]
}